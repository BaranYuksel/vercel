{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static",
      "config": {
        "zeroConfig": true
      }
    }
  ],
  "rewrites": [
    {
      "source": "/hello",
      "destination": "index.html"
    },
    {
      "source": "/status-rewrite-404",
      "destination": "index.html",
      "status": 404
    }
  ]
}
